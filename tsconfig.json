{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "scripthost",
            "es5",
            "es2015.promise"
        ],
        "sourceMap": false,
        "noImplicitAny": false,
        "preserveConstEnums": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "removeComments": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "paths": {
            "src/*": [
                "src/*"
            ],
            "component/*": [
                "src/components/*"
            ]
        }
    }
}
